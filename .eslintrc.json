{
  "parser": "vue-eslint-parser",
  "plugins": [
    "vue",
    "jest"
  ],
  "extends": [
    "teppeis/es2019",
    "teppeis/+typescript",
    "teppeis/+prettier",
    "plugin:vue/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "jestPuppeteer": true
  },
  "overrides": [{
      "files": ["*.js"],
      "parserOptions": {
        "parser": "babel-eslint"
      }
    },
    {
      "files": ["*.ts"],
      "parserOptions": {
        "parser": "@typescript-eslint/parser"
      }
    }
  ]
}