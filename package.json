{
  "name": "boilerplate_vueweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "output": "./dist",
    "staticSrc": "./static/**/{*.html,*.jpg,*.png,*.gif,*.mp4,*.mov,*.json,*.eot,*.svg,*.ttf,*.woff}",
    "cssSrc": "./src/styles/index.styl",
    "cssDest": "./dist/app.css",
    "webpackConfig": "./webpack/webpack.config.js"
  },
  "scripts": {
    "prestart": "run-s del mkdir",
    "start": "run-p cp:static:dev server build:*:dev open",
    "build": "run-s del cp:static:prod build:*:prod",
    "build:js:dev": "webpack --mode development --config=$npm_package_config_webpackConfig --watch",
    "build:js:prod": "webpack --mode production --config=$npm_package_config_webpackConfig",
    "build:html:prod": "pug ./src/templates --basedir ./src/templates/ --pretty --out $npm_package_config_output",
    "build:css:dev": "stylus --include-css $npm_package_config_cssSrc --out $npm_package_config_cssDest --use autoprefixer-stylus --watch",
    "build:css:prod": "stylus --include-css $npm_package_config_cssSrc --out $npm_package_config_cssDest --use autoprefixer-stylus --compress",
    "cp:static:dev": "cpx $npm_package_config_staticSrc $npm_package_config_output --watch",
    "cp:static:prod": "cpx $npm_package_config_staticSrc $npm_package_config_output",
    "lint": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "test": "jest --silent",
    "e2e": "npm run build && jest --config=jest.e2e.config.js",
    "test:update-snapshot": "jest --updateSnapshot",
    "precommit": "lint-staged",
    "server": "node ./server/index.js",
    "open": "opn http://localhost:3000",
    "mkdir": "mkdirp $npm_package_config_output",
    "del": "rimraf $npm_package_config_output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baco16g/boilerplate_vueweb.git"
  },
  "lint-staged": {
    "src/scripts/**/*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "author": "yuki.kobayashi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/baco16g/boilerplate_vueweb/issues"
  },
  "homepage": "https://github.com/baco16g/boilerplate_vueweb#readme",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "gsap": "^3.6.0",
    "vue": "^2.6.10",
    "vue-touch": "^2.0.0-beta.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@types/expect-puppeteer": "^3.3.1",
    "@types/gsap": "^1.20.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^24.0.13",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/puppeteer": "^1.12.4",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/babel-preset-app": "^3.8.0",
    "acorn": "^6.1.1",
    "autoprefixer-stylus": "^0.14.0",
    "babel-eslint": "^10.0.1",
    "cpx": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-teppeis": "^9.6.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-vue": "^5.2.2",
    "expect-puppeteer": "^4.1.1",
    "express": "^4.17.1",
    "husky": "^1.2.1",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "jest-puppeteer": "^4.2.0",
    "lint-staged": "^8.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^4.0.0",
    "pptr-testing-library": "^0.4.0",
    "prettier": "^1.17.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "puppeteer": "^1.17.0",
    "serve": "^11.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  }
}
